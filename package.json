{
    "name": "king-typer",
    "version": "0.0.1",
    "scripts": {
        "format": "prettier --write \"**/*.+(js|jsx|json|ts|tsx)\"",
        "dev": "pnpm recursive run dev --filter @king-typer/* --workspace-concurrency Infinity",
        "build": "pnpm run build --filter @king-typer/*",
        "start": "pnpm run start --filter @king-typer/*",
        "test": "pnpm run test --filter @king-typer/*",
        "lint": "eslint ./packages/**/*.{ts,tsx}",
        "check": "pnpm run format && pnpm run lint",
        "semantic-release": "semantic-release"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@types/node": "^12.12.24",
        "@typescript-eslint/eslint-plugin": "^2.15.0",
        "@typescript-eslint/parser": "^2.15.0",
        "all-contributors-cli": "^6.14.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-emotion": "^10.0.27",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.17.0",
        "prettier": "^1.19.1",
        "semantic-release": "^17.0.4",
        "tslib": "^1.10.0",
        "typescript": "^3.7.4"
    }
}
